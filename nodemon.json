{
    "watch": [
        "modules",
        "middleware",
        "config",
        "app.js",
        "server.js"
    ],
    "ext": "js",
    "ignore": [
        "node_modules",
        "uploads",
        ".git"
    ],
    "exec": "node",
    "delay": 500,
    "legacyWatch": true,
    "env": {
        "NODE_ENV": "development"
    }
}